from datetime import date

def main():

#set up another infinite loop to let user see options :0    
    reminders = []  
    while True:
        
        print("Menu:")
        
        print("1. Add a expense üêµ")
        
        print("2. Show your expenses üèãÔ∏è")
        
        print("3. Remove an expense üéØ")
        
        print("4. Exit üö®")

        user_choice = input("Choose an option: ")

        if user_choice == "1":
           
            print("Enter an expense: ")
            
            reminder_date = user_input()
            
            event_text = input("Enter the event description: ")
            

            reminders.append((reminder_date, event_text))
           
            reminders.sort(key=lambda x: x[0])

            print("Reminder added and sorted!")

        elif user_choice == "2":
            
            if not reminders:
               
                print("No reminders added yet.")
            
            else:
                
                print("Your reminders:")
                
                for i, (r_date, r_text) in enumerate(reminders, start=1):
                    
                    print(f"{i}. {r_date}  -  {r_text}")

